@model List<Order>

<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Done</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">ExpectedDate</th>
    </tr>
  </thead>
  <tbody>
  
  @foreach (var order in Model)
  {
    <tr>
      <th scope="row">@order.done</th>
      <td>@order.name</td>
      <td>@order.email</td>
      <td>@order.expectedDate</td>
    </tr>
  }

  </tbody>
</table>

